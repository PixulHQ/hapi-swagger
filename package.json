        "swagger-parser": "4.0.2"
    },
    "devDependencies": {
        "@hapi/basic": "^5.1.0",
        "@hapi/code": "^5.3.1",
        "@hapi/good": "^8.2.0",
        "@hapi/good-console": "^8.1.0",
        "@hapi/good-squeeze": "^5.2.0",
        "@hapi/h2o2": "^8.3.0",
        "@hapi/hapi": "^18.3.1",
        "@hapi/inert": "^5.2.0",
        "@hapi/lab": "^18.1.2",
        "@hapi/vision": "^5.5.2",
        "@hapi/wreck": "^15.0.1",
        "babel-eslint": "^10.0.1",
        "blipp": "^3.0.0",
